+static bool is_hummingboard(void);
+
 int board_init(void)
 {
 	/* address of boot parameters */
 	gd->bd->bi_boot_params = CONFIG_SYS_SDRAM_BASE + 0x100;
+
+	if (is_hummingboard())
+		gd->bd->bi_arch_number = 4773; /* HummingBoard machine ID */
+	else
+		gd->bd->bi_arch_number = 4821; /* CuBox-i machine ID */
 
 	return 0;
 }
